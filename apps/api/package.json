    "dotenv": "^16.4.5",
    "pg": "^8.12.0",
    "@prisma/client": "^5.20.0",
    "ioredis": "^5.4.2",
    "bullmq": "^5.30.1",
    "aws-sdk": "^2.1691.0",
    "@aws-sdk/client-s3": "^3.666.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
